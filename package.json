{
  "name": "thats",
  "version": "0.0.1",
  "description": "The most ambitious and smallest validation package of the World.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "transpile": "rimraf './dev/**/*' && ./node_modules/.bin/cross-env BABEL_ENV=transpile ./node_modules/.bin/babel src --out-dir dev",
    "minify": "rimraf './dist/**/*' && ./node_modules/.bin/cross-env BABEL_ENV=minify ./node_modules/.bin/babel dev --out-dir dist",
    "build": "npm run transpile && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marhc/thats.git"
  },
  "keywords": [
    "validation",
    "validate",
    "util",
    "utilities",
    "duck-typing",
    "type",
    "type-checking"
  ],
  "author": "Marcio Coutinho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marhc/thats/issues"
  },
  "homepage": "https://github.com/Marhc/thats#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-preset-minify": "^0.5.0",
    "benchmark": "^2.1.4",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "rimraf": "^2.6.3"
  },
  "dependencies": {}
}
